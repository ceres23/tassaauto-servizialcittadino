{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false,
        "loose": false,
        "targets": "> 1%, last 2 versions, not ie <= 10",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "comments": false
}